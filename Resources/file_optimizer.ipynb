    "import cv2\n",
    "\n",
    "folder_path = \"..\\Dataset\\person1\"\n",
    "\n",
    "for filename in os.listdir(folder_path):\n",
    "    img_path = os.path.join(folder_path, filename)\n",
    "    img = cv2.imread(img_path)\n",
    "    height, width, channels = img.shape\n",
    "    if height != width:\n",
    "        os.remove(img_path)\n"
